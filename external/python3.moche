[Repo]
    Name                                python3
    VersionMin                          3.5.0
    VersionCheckExecutable              python
    VersionCheckArguments               --version
    VersionCheckRegex                   ^Python ([0-9]+(\.[0-9]+)*)$
    [Retrieval]
        Name                            path
        [Path]
    [Retrieval]
        Name                            path-root
        [Path]
            Path                        C:\Python35
            SkipVersionCheck            True
                    
[Tool]
    Name                                python3
    Repo                                python3
    Executable                          python
