[Repo]
    Name                                python3
    VersionMin                          3.5.0
    VersionCheckExecutable              python
    VersionCheckArguments               --version
    VersionCheckRegex                   ^Python ([0-9]+(\.[0-9]+)*)$
    [Retrieval]
        Name                            path
        [Path]
    [Retrieval]
        Name                            path-root
        Condition                       IsWindows
        [Path]
            Path                        C:\Python35
    [Retrieval]
        Name                            official-release
        Version                         3.5.2
        Branch                          python.org
        [Binary]
            [Url]
                [Win32]
                    Url                 https://www.python.org/ftp/python/3.5.2/python-3.5.2-embed-amd64.zip
                [Win64]
                    Url                 https://www.python.org/ftp/python/3.5.2/python-3.5.2-embed-win32.zip
                    
[Tool]
    Name                                python3
    Repo                                python3
    Executable                          python
