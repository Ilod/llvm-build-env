[Repo]
    Name                                python3
    VersionMin                          3.4.0
    VersionCheckExecutable          python
    {OS} linux
        VersionCheckExecutable          python3
    VersionCheckArguments               --version
    VersionCheckRegex                   ^Python ([0-9]+(\.[0-9]+)*)$
    [Retrieval]
        Name                            path
        [Path]
    [Retrieval]
        Name                            path-root
        [Path]
            Path                        C:\Python35
    [Retrieval]
        Name                            ilod-mirror
        Version                         3.5.2
        Branch                          ilod.org
        [Binary]
            [File]
                {Platform} win64
                    Url                 http://ilod.org/python-3.5.2-x64.zip
                {Platform} win32
                    Url                 http://ilod.org/python-3.5.2-x86.zip
                    
[Tool]
    Name                                python3
    Repo                                python3
    Executable                          python
    {OS} linux
        Executable                      python3
